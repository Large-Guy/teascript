class Bird
{
    new() {}

    function fly()
    {
        print("the bird can fly")
    }
}

class Parrot : Bird
{
    function speak()
    {
        print("the parrot can speak")
    }
}

class Eagle : Parrot
{
    function vision()
    {
        print("The eagle has a sharp vision")
    }
}

var eagle = Eagle.new()
eagle.speak()
eagle.fly()
eagle.vision()