    c = "orange",
    bar = 
    {
        d = "purple",
        e = "blue",
    },
}
print(foo)      // Expect: {a = red, b = yellow, c = orange, bar = {d = purple, e = blue}}

print(foo.bar.d)    // Expect: purple

