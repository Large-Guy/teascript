function guess()
{
    var guess = 17

    while 
    {
        var n = number(input("Input your guess: "))
        if(n == guess) 
        {
            print("Well done!");
            return
        } 
        else if(n < guess) 
        {
            print("Too low!");
        } 
        else 
        {
            print("Too high!");
        }
    }
}

guess()