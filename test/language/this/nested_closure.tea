class Foo 
{
    new() {}
    function getClosure() 
    {
        function f() 
        {
            function g() 
            {
                function h() 
                {
                    return self.toString()
                }
                return h
            }
            return g
        }
        return f
    }

    function toString() { return "Foo" }
}

var closure = Foo.new().getClosure()
print(closure()()()) // expect: Foo