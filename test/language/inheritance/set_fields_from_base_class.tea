class Foo 
{
    foo(a, b) 
    {
        self.field1 = a
        self.field2 = b
    }

    fooprint() 
    {
        print(self.field1)
        print(self.field2)
    }
}

class Bar : Foo 
{
    bar(a, b) 
    {
        self.field1 = a
        self.field2 = b
    }

    barprint() 
    {
        print(self.field1)
        print(self.field2)
    }
}

var bar = Bar.new()
bar.foo("foo 1", "foo 2")
bar.fooprint()
// expect: foo 1
// expect: foo 2

bar.bar("bar 1", "bar 2")
bar.barprint()
// expect: bar 1
// expect: bar 2

bar.fooprint()
// expect: bar 1
// expect: bar 2
