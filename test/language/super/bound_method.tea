class A 
{
    method(arg) 
    {
        print("A.method(" + arg + ")")
    }
}

class B : A 
{
    new() {}
    getClosure()
    {
        return super.method
    }

    method(arg) 
    {
        print("B.method(" + arg + ")")
    }
}


var closure = B.new().getClosure()
closure("arg"); // expect: A.method(arg)
