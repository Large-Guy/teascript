class Base 
{
    function method() 
    {
        print("Base.method()")
    }
}

class Derived : Base 
{
    new() {}
    function method() 
    {
        super.method()
    }
}

class OtherBase 
{
    function method() 
    {
        print("OtherBase.method()")
    }
}

var derived = Derived.new()
derived.method() // expect: Base.method()
Base = OtherBase
derived.method() // expect: Base.method()