class Base 
{
    function toString() { return "Base" }
}

class Derived : Base 
{
    new() {}
    function getClosure() 
    {
        function closure() 
        {
            return super.toString()
        }
        return closure
    }

    function toString() { return "Derived" }
}

var closure = Derived.new().getClosure()
print(closure()) // expect: Base