class A 
{
    say() 
    {
        print("A")
    }
}

class B : A 
{
    getClosure() 
    {
        function closure() 
        {
            super.say()
        }
        return closure
    }

    say() 
    {
        print("B")
    }
}

class C : B 
{
    new() {}
    say() 
    {
        print("C")
    }
}

C.new().getClosure()() // expect: A
