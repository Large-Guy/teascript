class Foo 
{
    new(arg) 
    {
        print("Foo.new(" + arg + ")")
        this.field = "new"
    }
}

var foo = Foo("one") // expect: Foo.new(one)
foo.field = "field"

var foo2 = foo.new("two") // expect: Foo.new(two)
print(foo2) // expect: <Foo instance>

// Make sure new() doesn't create a fresh instance
print(foo.field) // expect: new
