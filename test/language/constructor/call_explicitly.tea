class Foo 
{
    new(arg) 
    {
        print("Foo.new(" + arg + ")")
        self.field = "new"
    }
}

var foo = Foo.new("one") // expect: Foo.new(one)
foo.field = "field"

print(foo) // expect: <Foo instance>

// Make sure new() doesn't create a fresh instance
print(foo.field) // expect: field
