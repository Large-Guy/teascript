class Foo 
{
    function inFoo() 
    {
        print("in foo")
    }
}

class Bar : Foo 
{
    function inBar()
    {
        print("in bar")
    }
}

class Baz : Bar 
{
    new() {}
    function inBaz()
    {
        print("in baz")
    }
}

var baz = Baz.new()
baz.inFoo() // expect: in foo
baz.inBar() // expect: in bar
baz.inBaz() // expect: in baz
